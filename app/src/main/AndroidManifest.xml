<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dbz.niobiopools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_niobio"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_niobio"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.dbz.niobiopools.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.dbz.niobiopools.ManageModelActivity"
            android:label="@string/title_activity_add_address"
            android:parentActivityName="com.dbz.niobiopools.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.dbz.niobiopools.MainActivity" />
        </activity>
    </application>

</manifest>